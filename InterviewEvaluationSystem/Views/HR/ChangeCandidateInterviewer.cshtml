
@{
    ViewBag.Title = "ChangeCandidateInterviewer";
}

<h2>ChangeCandidateInterviewer</h2>
@{ 
    var grid = new WebGrid(ViewBag.notificationList, canPage: true, rowsPerPage: 5, canSort: true);
}

            @grid.GetHtml(tableStyle: "table table-bordered table-striped table-condensed",
        headerStyle: "webgrid-header",
        footerStyle: "webgrid-footer",
        alternatingRowStyle: "webgrid-alternating-row",
        selectedRowStyle: "webgrid-selected-row",
        rowStyle: "webgrid-row-style",
        mode: WebGridPagerModes.All,
        columns: grid.Columns(
            @*grid.Column("CandidateID", format:@<text><span><label id="lblCandidateID">@item.CandidateID</label></span></text>),*@
            grid.Column("Name", format:@<text><span><label id="lblCandidateName">@item.Name</label></span></text>),
            grid.Column("Email", format:@<text><span><label id="lblCandidateEmail">@item.Email</label></span></text>),
            grid.Column("RoundID","Upcoming Round", format:@<text><span><label id="lblCandidateRound">@item.RoundID</label></span></text>),
            grid.Column("","Assigned Interviewer"),
            grid.Column("Action", format: @<text>
                <button class="edit-user display-mode btn btn-default" id="@item.CandidateID">Edit</button>
                
            </text>))
            )